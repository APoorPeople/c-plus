#include<iostream.h>
void PrintStars(int m,int n,char c)
{
	int i,j;
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			cout<<c;
		}
		cout<<endl;
	}
}
void main()
{
	int x,y;
	char ch;
	cin>>x>>y>>ch;

	cout<<x<<endl;
	PrintStars(x,y,ch);

	cout<<y<<endl;
	PrintStars(x,y,ch);

	
	cout<<ch<<endl;
	PrintStars(x,y,ch);
}
